	}
}

function search(sessionid) {
	var sTerm = document.forms["rightnavselektor"].searchTerm.value;

	sTerm = encodeurl(sTerm);

	if(sTerm==""||sTerm==" " ||sTerm.length<3) {
		alert("Bitte Suchbegriff eingeben (mindestens 3 Zeichen)!");

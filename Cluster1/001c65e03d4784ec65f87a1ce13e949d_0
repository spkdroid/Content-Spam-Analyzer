<script type="text/javascript">
function google_ad_request_done(google_ads) {
	var i;
	var td = 0;
	var width = 100;
	
	if (google_ads.length > 0) {
		document.write('<table width="100%" border="0" cellspacing="0" cellpadding="0" class="google_border">');
		document.write('<tr><td class="google_header" colspan=' + td + '>SPONSORED LINKS</td></tr>');
		document.write('<tr>');
		document.write('<td>');
		document.write('<table width="100%" border="0" cellspacing="0" cellpadding="0">');
		document.write('<tr>');
		
		if (google_ads.length > 1) {
			var width = (100 - ((google_ads.length - 1) * 1)) / google_ads.length;
		}
	
		for(i = 0; i < google_ads.length; ++i) {
			if (i > 0) {
				document.write('<td align="center" valign="top" class="google_separator" width="1%"></td>');
				td++;
			}
			
			document.write('<td class="google_adbox" width="' + width +'%">');
			document.write('<nobr><a href="' + google_ads[i].url + '" class="google_link" target=_blank>' + google_ads[i].line1 + '</a></nobr><br >');
			document.write(google_ads[i].line2 + ' ' + google_ads[i].line3 + '</br>');
			document.write('</td>');
			td++;
		}
		
		document.write('</tr>');
		document.write('<tr><td class="google_footer" colspan=' + td + '><div align="right"><span class="google_footer_b">What is a Sponsored Link?</span> Provided by a third party and not endorsed by Time Inc.&nbsp;</div></td></tr>');
		document.write('</table>');
		document.write('</td>');
		document.write('</tr>');
		document.write('</table>');
	}
}
	
google_safe = 'high';
google_ad_client = 'timeincint_parenting_js';
google_ad_output = 'js';
google_max_num_ads = '4';
</script>

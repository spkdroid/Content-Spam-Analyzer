  <script language="javascript" type="text/javascript">
  var DIST_TO_TOP = 130;
  var DIST_TO_LEFT = 50;
  /**
   * Catch scrolling
   */
  if(document.all)
  {
    //window.onscroll = MoveDiv;
    document.getElementById("popup_blocker_div").style.position = "absolute";
    /**
     * Check if scrolling happened, if so, move div to proper location
     *
     *if(document.getElementsByTagName("body")[0].scrollTop>10)
     * MoveDiv();
     */
  }
  
  /**
   * onScroll method
   */
  function MoveDiv()
  {
    var scrollDist = document.getElementsByTagName("body")[0].scrollTop;
    document.getElementById("popup_blocker_div").style.top = (DIST_TO_TOP + scrollDist) + "px";
  }

  /**
   * Function hides all form elements below popup prior to popup, then reverses process when popup closes
   */
  function hideHTMLElements()
  {
  //document.getElementById('popup_blocker_div').style.visibility='hidden';return false;
   tags = document.getElementsByTagName("select"); // all tags
   if(document.getElementById("popup_blocker_div").style.visibility=="hidden")
   {
    for(i=0; i< tags.length; i++)
    {
      tags[i].style.visibility = "hidden";
    }
    document.getElementById("popup_blocker_div").style.visibility = "visible";
   }
   else
   {
    for(i=0; i< tags.length; i++)
    {
      tags[i].style.visibility = "visible";
    }
    document.getElementById('popup_blocker_div').style.visibility = "hidden";
   }
   return false;
  }
  /**
   * Function resets cookies to empty
   */
  function ResetCookies()
  {
    if(domainName == "")
    {
      document.cookie = 'early_exit=0; path=/ ';
      document.cookie = 'f_qnote=0; path=/ ';
      document.cookie = 'laststep=' + laststep + '; path=/ ';
      document.cookie = 'step=' + step + '; path=/ ';
      document.cookie = 'process=; path=/ ';
      document.cookie = 'msg_id=0; path=/ ';
    }
    else
    {
      document.cookie = 'early_exit=0; path=/; domain=.classmates.com';
      document.cookie = 'f_qnote=0; path=/; domain=.classmates.com';
      document.cookie = 'laststep=' + laststep + '; path=/; domain=.classmates.com';
      document.cookie = 'step=' + step + '; path=/; domain=.classmates.com';
      document.cookie = 'process=; path=/; domain=.classmates.com';
      document.cookie = 'msg_id=0; path=/; domain=.classmates.com';
    }
  }
  
    var selWinHeight = 0;
    var selWinWidth = 0;
    var selWinTop = DIST_TO_TOP; // how far from top to display either
    var selWinLeft = DIST_TO_LEFT; // how far from left to display either
    var selWinTitle = "";
    var winBackgroundColor = "#cccc99"; // default
    var req = false; // HTTP XML request object
    if ( 
        document.URL.indexOf("terms.jsp") == -1 &&
        document.URL.indexOf("privacy.jsp") == -1 )
    {
      isMac = (navigator.appVersion.indexOf("Mac") != -1) ? true : false;
      IEMac = ((document.all) && (isMac)) ? true : false;
      var early_exit_pop;
      var laststep;
      var step;
      var process;
      var failQnote;
      var msgId;
      var args = "";
      var pre_url = "";

      var pop_content_path = "";

      var domainName = ".classmates.com";
      if (IEMac)
      {
        var popIndex = document.cookie.indexOf("early_exit");
        var laststepIndex = document.cookie.indexOf("laststep");
        var stepIndex = document.cookie.indexOf("step");
        var processIndex = document.cookie.indexOf("process=");
        var failQnoteI = document.cookie.indexOf("f_qnote=");
        var msgIdIndex = document.cookie.indexOf("msg_id=");
        early_exit_pop = document.cookie.substr(popIndex+12,1);
        laststep = document.cookie.substr(laststepIndex+12,1);
        step = document.cookie.substr(stepIndex+12,1);
        process = document.cookie.substr(processIndex+8,1);
        failQnote = document.cookie.substr(failQNoteI+7,1);
        msgId = document.cookie.substr(msgIdIndex+12,1);
      }
      else
      {
        early_exit_pop = ReadCookie('early_exit');
        laststep = ReadCookie('laststep');
        step = ReadCookie('step');
        process = ReadCookie('process');
        failQnote = ReadCookie('f_qnote');
        msgId = ReadCookie('msg_id');
      }

      var showPop = false;
      if (step=="catalog" && (document.URL.indexOf("/checkout/") < 0)) {showPop = true;} 
      if (laststep=="payment" && step=="catalog") {showPop = true;}
      
      if ((early_exit_pop > 0) && (showPop) && (document.URL.indexOf("thankyou") < 0))
      {
        if(document.URL.indexOf('https://')>-1)
        {
          pop_content_path = '/cmo/user/checkout/earlyexit.jsp';
        }
        else
        {
          pop_content_path = '/cmo/popup/checkout/earlyexit.jsp';
        }
        args = "?step=" + step + "&process=" + process;

        ResetCookies();

        if(failQnote==1)
        {
          
          selWinHeight = 225;
          selWinWidth = 300;
          winBackgroundColor = "#ffffff";
          selWinTitle = "Don't Stop Now";
          args = "?quicknotes=true";
        }
        else if(process == "renewal") 
        {
          
          selWinHeight = 600;
          selWinTitle = "Don't Stop Now";
          selWinWidth = 400;
        }
        else if(process == "cc2p2test3") 
        {
          
          selWinTop = 100;
          selWinLeft = 80;
          selWinHeight = 415;
          selWinWidth = 263;
          selWinTitle = " ";
          winBackgroundColor = "#cbe47f";
          args = "?cc2p2test3=true&messageId=" + msgId;
          document.getElementById("popup_blocker_table").style.backgroundColor = "#99cc00";
          document.getElementById("closeLink").innerHTML = '<a href="' + "/cmo/user/checkout/confirmation.jsp?messageId=" +
                                                           msgId + '" onclick="document.getElementById(\'popup_blocker_div\').style.visibility=' +
                                                           '\'hidden\'"><span style="font-size:14px;font-weight:bold;color:#ffffff">X</span></a>';
        }
        else 
        {
          selWinHeight = 580;
          selWinTitle = "Don't Stop Now";
          selWinWidth = 350;
        }

        finalURL = (pop_content_path+args);
        if(document.URL.indexOf('https://')>-1)
        {
          var currURL = document.location + "";
          var tmpDomain = "";
          var tmpPort = "";
          var len = currURL.indexOf("/cmo");
          tmpDomain = currURL.substr(0,len); // all domain and such
          pre_url = tmpDomain + finalURL;
          finalURL = pre_url;
        }
        result = window.open(finalURL, "DontStop","toolbar=0,menubar=0,scrollbars=no,scrolling=no,resizable=no,width=" + selWinWidth + ",height=" + selWinHeight + ",top=" + selWinTop + ",left=" + selWinLeft + "");

        if(result!=null)
        {
          result.focus();
        }
        else
        {
          if(document.all)
            req = new ActiveXObject("Microsoft.XMLHTTP");
          else
            req = new XMLHttpRequest();
          if(req)
          {
              req.onreadystatechange = processReqChange; // function to handle data
              req.open("GET", (pop_content_path+args));
              req.send(null);
          }
        }
      }// end of determine to pop or not
      else if(document.URL.indexOf("thankyou") >= 0) // we have the thank you page
      {
        // reset the cookies
        ResetCookies();
      }
    }

    function processReqChange() 
    {
      if (req.readyState == 4) 
      {
        if (req.status == 200) 
        {
          document.getElementById("popup_blocker_html").innerHTML = req.responseText;
          document.getElementById("popup_blocker_win_title").innerHTML = selWinTitle;
          document.getElementById("popup_blocker_div").style.top = selWinTop + "px"; // get it away from the edge some
          document.getElementById("popup_blocker_div").style.left = selWinLeft + "px";
          selWinHeight -= 50; // offset by no popup win
          document.getElementById("popup_blocker_div").style.width = selWinWidth + "px";
          document.getElementById("popup_blocker_div").style.height = selWinHeight + "px";
          document.getElementById("popup_blocker_html").style.backgroundColor = winBackgroundColor;
          setTimeout("hideHTMLElements()",100);
        }
      }
    }
  </script>







  
  




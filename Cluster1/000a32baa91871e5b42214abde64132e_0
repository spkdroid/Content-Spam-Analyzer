function cm_grab_cookie(cookie_name) {
  var ac = document.cookie;
  var pos = ac.indexOf(cookie_name);
  var loc = document.location;
  var value;

  if(pos != -1) {
    var start = pos + cookie_name.length;
    var end = ac.indexOf(";", start);
    if(end == -1) end = ac.length;
    value = ac.substring(start, end);
    value = unescape(value);
  }

  return value;
}


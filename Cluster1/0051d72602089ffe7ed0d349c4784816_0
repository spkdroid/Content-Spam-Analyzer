<script language="javascript">
function FormCheck()
{
	if (form.성명.value == "") { alert("이름을 입력해주세요"); form.성명.focus(); return; }
	if (form.성명.value.length < 2) { alert("이름은 2자 이상으로 쓰셔야 합니다"); form.성명.focus(); return; }
				
	for (var j = 0; j < form.성명.value.length; j++) {
		if (form.성명.value.charAt(j) == " ") {
			alert("이름에는 공백이 들어가면 안됩니다");
			form.성명.focus();
			return;
		} else if (form.성명.value.charAt(j) <= "\u00A1") {
			alert("이름은 한글만 가능합니다");
			form.성명.focus();
			return;
			}
		}
					
	if (form.주민번호.value == "") { alert("주민등록번호를 입력해주세요"); form.주민번호.focus(); return; }
	if (form.주민번호2.value == "") { alert("주민등록번호를 입력해주세요"); form.주민번호2.focus(); return; }
	if ((form.주민번호.value == "") || (form.주민번호2.value == "")) { alert("주민등록번호를 입력해주세요"); form.주민번호.focus(); return; }

	if ((form.handphone.value == "") || (form.handphone2.value == "")) {
		alert("핸드폰번호를 입력해주세요"); form.handphone.value = ""; form.handphone2.value = ""; form.hp.focus(); return;
	}
	if ((form.handphone.value.length < 3) || (form.handphone2.value.length < 4)) {
	alert("핸드폰번호를 올바르게 입력해주세요"); form.handphone.value = ""; form.handphone2.value = ""; form.hp.focus(); return;
	}		
	
	if ((form.phone.value == "") || (form.phone2.value == "") || (form.phone3.value == "")) {
		alert("전화번호를 입력해주세요"); form.phone.value = ""; form.phone2.value = ""; form.phone3.value = ""; form.phone.focus(); return;
	}
	if ((form.phone.value.length < 2) || (form.phone2.value.length < 3) || (form.phone3.value.length < 4)) {
	alert("전화번호를 올바르게 입력해주세요"); form.phone.value = ""; form.phone2.value = ""; form.phone3.value = ""; form.phone.focus(); return;
	}
	if (form.주소.value == "") { alert("주소를 입력해주세요"); form.주소.focus(); return; }
	if (form.카드종류.value == "") { alert("카드종류를 입력해주세요"); form.카드종류.focus(); return; }
	if (form.연체금액.value == "") { alert("귀하의 연체금액을 입력해주세요"); form.연체금액.focus(); return; }
	if (form.결제일.value == "") { alert("결제일을 입력해주세요"); form.결제일.focus(); return; }
	if (form.대출희망금액.value == "") { alert("대출희망금액을입력해주세요"); form.대출희망금액.focus(); return; }
		
	form.submit();
	}
		//-->
</script>

function swapImage(imgName,imgPath){
	if(document.images){
		oImg = eval("document." + imgName);
		if(oImg){
			oImg.src = imgPath;
		}
	}
}


function popupDisplay(){
	var myForm = document.signup;
	var email = myForm.header_email.value;
	emailPopUp = window.open('../service/email_signup_popup.asp?cmid=ftr_email&header_email=' + escape(email) ,'emailWin','width=506,height=580,scrollbars,resizable');
	document.signup.header_email.value = "Enter your email address here";
}


function blankEmail() {
		if (document.signup.header_email.value == "Enter your email address here") {
			document.signup.header_email.value = "";
		}
}


function blankSearch( searchLoc ) {
    if ( searchLoc == "H" )
    {
      if (document.searchForm.search_words.value == "Type keyword(s) or item #") {
        document.searchForm.search_words.value = "";
      }
    }
    else if ( searchLoc == "F" )
    {
      if (document.searchFormF.search_words.value == "Type keyword(s) or item #") {
        document.searchFormF.search_words.value = "";
      }
    }
}

function printerFriendly() {
     if (window.print) {
          window.print();
     } else {
          alert("Pease use File >> Print in order to print a printer friendly page.");
     }
}


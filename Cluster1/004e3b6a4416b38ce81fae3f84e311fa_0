			function NetscapeEventHandler_KeyDown(e) {
				if (e.which == 13 && e.target.type != 'textarea' && e.target.type != 'submit') {
					//alert(e.target.name);
					if (e.target.name == '_ctl0:_ctl4:txtQuery') {

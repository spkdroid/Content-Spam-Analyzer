<!-- Advertiser Cookie Deploy -->
<!-- This is a 24 hour cookie that delivers the creative once a day.  
This cookie code to be entered in separate bogus order such as Order #4156 Pop cookie check, 
one flight matching each creative flight to be entered for advertiser.  Enter code only as an html creative without html or body tags.
Separate creative code should be entered in order under advertiser's name.  Pop cookie serves original code which checks 24 hour status.
If time for delivery, the script makes the ad call to the advertiser's creative popWindow, also set up as html ad, but under the advertiser's name. 
See line notes below marked with // which javascript ignores -->

<script language=javascript>
var domain = "dailypress.com";
// 1440 sets the 24 hour timer.  To test, change to 0.1440 and it will always work.
var min_timeout = 480;
// AdvertiserName_popunder repeated twice, must be identical.  HERE...
expired = document.cookie.indexOf("ecpi_Popunder");
var enabled = testCookie()
if (!enabled) {
expired = 0;
}
if (expired == -1) {
setCookie(min_timeout);
popUnder();
}
function setCookie(min_timeout) {
var d = new Date();
var timeout = d.getTime() + (min_timeout * 60 * 1000);
var expires = new Date();
expires.setTime(timeout);
var stamp = d.valueOf();
if (document.domain.indexOf(domain) == -1) {
domain = "";

}if (domain.length > 0 ) {
	domain = "domain=." + domain + ";";
}
var expires_str = expires.toGMTString();
// AdvertiserName_popunder repeated twice, must be identical.  And HERE...
var cookie_str = "ecpi_Popunder=" + stamp + ";path=/;"
cookie_str +=  domain + "expires=" + expires_str;
document.cookie = cookie_str;
}
function testCookie() {
var enabled = navigator.cookieEnabled
//Netscape: cookieEnabled is not available for Netscape.
//Test for cookies enabled by setting and getting a cookie
if (enabled == null) {
var d = new Date();
var timeout = d.getTime() + (1 * 60 * 1000);
var expires = new Date();
expires.setTime(timeout);
var expires_str = expires.toGMTString();
// AdvertiserNamePopUnderCookie repeated twice, must be identical.  HERE...
var test_str = "ecpiPopUnderCookie=" + 1 + ";path=/;"
test_str +=  domain + "expires=" + expires_str;
document.cookie = test_str;
// AdvertiserNamePopUnderCookie repeated twice, must be identical.  And HERE...
enabled = document.cookie.indexOf("ecpiPopUnderCookie");
if (enabled == -1) {
return 0;
}
}
//Explorer: Use cookieEnabled property
else {
if (enabled==0) {
return 0;
}
}
return 1;
}
function popUnder() {
// Set ad call HERE...
var page = "http://brt.trb.com/html.ng/site=dailypress&adplacement=15&adtype=popwindow";
// Set window properties including width and height HERE...
var windowprops = "resizable=no,width=600,height=250,status=no,location=no,toolbar=no";
var popwin = window.open(page,'ecpi', windowprops);
popwin.opener.focus();
}
</script>


function changeArrow(sortStr,id){FID=document.getElementById(id);FID.style.color='#FFFFCC';if (arrowStr == sortStr) {FID.style.color='#D2FFD2';if (arrowDir=='asc') {window.document.write(' <img src=http://admin.et-mall.com.tw/image/sup.gif border=0 >');}if (arrowDir=='desc') {window.document.write(' <img src=http://admin.et-mall.com.tw/image/sdown.gif border=0 >');}}}

function chgInputType(){if (document.searchForm.PNameStr.selectedIndex==0) {document.getElementById('SNameStr').disabled=false;document.getElementById('SNameStr1').disabled=true;document.getElementById('SNameStr2').disabled=true;document.getElementById('sub0').style.display='block';document.getElementById('sub1').style.display='none';document.getElementById('sub2').style.display='none';}if (document.searchForm.PNameStr.selectedIndex==1) {document.getElementById('SNameStr').disabled=true;document.searchForm.SNameStr1[0].disabled=false;document.searchForm.SNameStr2[0].disabled=false;document.searchForm.SNameStr1[1].disabled=true;document.searchForm.SNameStr2[1].disabled=true;document.getElementById('sub1').style.display='block';document.getElementById('sub0').style.display='none';document.getElementById('sub2').style.display='none';}if (document.searchForm.PNameStr.selectedIndex==2) {document.getElementById('SNameStr').disabled=true;document.searchForm.SNameStr1[1].disabled=false;document.searchForm.SNameStr2[1].disabled=false;document.searchForm.SNameStr1[0].disabled=true;document.searchForm.SNameStr2[0].disabled=true;document.getElementById('sub2').style.display='block';document.getElementById('sub0').style.display='none';document.getElementById('sub1').style.display='none';}}
function checkSearch(inputObj){if (inputObj.PNameStr.value=='地點-行程') {if (inputObj.SNameStr.value=='') {alert('請輸入搜尋字串！');return false;}}if (inputObj.PNameStr.value=='價格') {if (inputObj.SNameStr1[0].value=='' || isNaN(inputObj.SNameStr1[0].value)) {alert('請輸入價格！必須為數字！');return false;}if (inputObj.SNameStr2[0].value=='' || isNaN(inputObj.SNameStr2[0].value)) {alert('請輸入價格！必須為數字！');return false;}}if (inputObj.PNameStr.value=='天數') {if (inputObj.SNameStr1[1].value=='' || isNaN(inputObj.SNameStr1[1].value)) {alert('請輸入天數！必須為數字！');return false;}if (inputObj.SNameStr2[1].value=='' || isNaN(inputObj.SNameStr2[1].value)) {alert('請輸入天數！必須為數字！');return false;}}return true;}

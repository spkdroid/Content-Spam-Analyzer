<script language="javascript">
function popcustom(url, name, gen, yuk, sbar, resize)
{
	window.open(url,name,"toolbar=0,location=0,directories=0,status=1,menubar=0,scrollbars=" + sbar + ",resizable=" + resize + ",width=" + gen + ",height=" + yuk + ",screenX=0,screenY=0,top=50,left=350")
}

  function displayStates(country_choice, state_choice) {

    var index = country_choice.selectedIndex;
    var option1 = country_choice.options[index].value;
    var iteSize = 0;
    var vec;

    option_list = state_choice.options;
    for (i=0; i < vecsize; i++) {
      option_list[0] = null;
    }

    if(option1 == "US"){
      iteSize = ii;
      vec = usaStates;
      var option = new Option("Nationwide", "N/A");
      option_list[0] = option;
    } else if(option1 == "CA"){
      iteSize = jj;
      vec = canadaStates;
      var option = new Option("Nationwide", "N/A");
      option_list[0] = option;
    } else {
      iteSize = kk;
      vec = defaultStates;
    }

    var isSelected = false;
    for (i = 0; i < iteSize; i++) {
      var params = vec[i].split("_");
      var len = option_list.length;
      var option = new Option(params[1], params[0]);
      option_list[len] = option;
    }
    if (option_list.length == 0){
      var option = new Option("N/A", "N/A");
      option_list[0] = option;
    }
    option_list[0].selected=true;
  }
</script>


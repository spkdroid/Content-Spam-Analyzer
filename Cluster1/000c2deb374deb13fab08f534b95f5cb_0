function doAdvancedSearch()
{
			var query = "?q=" + escape(document.searchForm.q.value).replace("+","%2b");

			query =  "http://tm.wc.teoma.com/r?t=n&s=m&id=30634&sv=za5cb0d00&uid=282cf753282cf7532&sid=382cf753282cf7532&p=%2Findex%2Easp&o=&u=http://s.teoma.com/AdvancedSearch" + query;
			window.location.href = query;
}


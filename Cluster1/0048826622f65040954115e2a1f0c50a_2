function datadisplay(productid,datatype) {
	width=400;
	height=200;
	urlstring='/onlinecatalog/ExtDisplay.cfm?Data='+datatype+'&productid='+productid;

	LeftPosition = (screen.width) ? (screen.width-width)/2 : 0;
	TopPosition = (screen.height) ? (screen.height-height)/2 : 0;

	win1=window.open(urlstring,'win1','top=' + TopPosition + ',left=' + LeftPosition + 'directories=no,hotkeys=no,location=no,menubar=no,status=no,toolbar=no,resizable=yes,scrollbars=yes,height=' + height +',width='+ width)
	win1.focus();
			
	 if (win1 != null && win1.opener == null)
       		      win1.opener=window;
}


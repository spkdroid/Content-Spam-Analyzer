<script language="JavaScript">
function gethex(num) {hex="0123456789ABCDEF";return hex.substr(num/16,1)+hex.substr((num&15),1)}
function getdec(num) {hex="0123456789ABCDEF";return hex.indexOf(num.toUpperCase().substr(0,1))*16+hex.indexOf(num.toUpperCase().substr(1,1))}

function fadeitem() {
	if (mcitem=="") {return}
	cr = Math.round(((rdif/100)*fade))+getdec(startcolor.substr(0,2));cg = Math.round(((gdif/100)*fade))+getdec(startcolor.substr(2,2));cb = Math.round(((bdif/100)*fade))+getdec(startcolor.substr(4,2))
	document.getElementById(mcitem).style.backgroundColor = "#"+gethex(cr)+gethex(cg)+gethex(cb)
	if (fade<100) {fade+=2;setTimeout("fadeitem()",60)} else {document.getElementById(mcitem).style.backgroundColor = "transparent"}
}

	fade = 0
	startcolor = "45ABFF"
	endcolor   = "FFFFFF"
	rdif = getdec(endcolor.substr(0,2))-getdec(startcolor.substr(0,2))
	gdif = getdec(endcolor.substr(2,2))-getdec(startcolor.substr(2,2))
	bdif = getdec(endcolor.substr(4,2))-getdec(startcolor.substr(4,2))
	mcitem = ""
	for (i=0;i<document.getElementsByTagName("TABLE").length;i++) {
		if (String(document.getElementsByTagName("TABLE")[i].id).indexOf("mcitem")!=-1) {
		 mcitem = document.getElementsByTagName("TABLE")[i].id
		}
	}
	
// this line initiates the fade on the cart 


</script>




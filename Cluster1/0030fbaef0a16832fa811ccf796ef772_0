function SubmitForm(theform)
{
if ((theform.EMAIL_ADDRESS.value == '') || (theform.EMAIL_ADDRESS.value == null))
{
   alert("You must enter an email address.")
   theform.EMAIL_ADDRESS.focus()
   theform.EMAIL_ADDRESS.select()
